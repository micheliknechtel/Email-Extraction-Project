    <!-- BEGIN ETIP TITLE -->
    <p><span class="blueheader">(98/Me) Clear Up the Confusion</span>
    <!-- END ETIP TITLE -->
    <br>
    <!-- BEGIN ETIP CONTENT -->    
    <style type="text/css">
li.bold {
  font-weight: bold;
